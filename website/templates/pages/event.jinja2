{% extends 'base.jinja2' %}

{% block header %}{% endblock %}
{% block footer %}{% endblock %}
{% block page_title %}Event{% endblock %}

{% block page_id %}event{% endblock %}
{% block body_class %}event{% endblock %}

{% block content %}
    <div class="inner">
        {{ macros.animated_ellipsis('Tracking event') }}
    </div>
{% endblock %}

{% block include_js %}
    <script>
        window.aseaman.ready(function(exports) {
            exports.event.track();
        });
    </script>
{% endblock %}
