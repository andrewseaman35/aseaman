{% extends 'base.jinja2' %}

{% block page_id %}compare-acnh{% endblock %}
{% block body_class %}compare-acnh{% endblock %}

{% block content %}
    <div class="inner">
        <h1>
            Compare ACNH
        </h1>
        <p>
            Choose your favorite Animal Crossing: New Horizons villager and help us find
            the ultimate villager!
        </p>
        <p>
            <strong>Note:</strong> the backend isn't set up yet, so it's not saving anything!
            Also, you shouldn't be here.. how did you find this page?
        </p>
        <div id="compare-acnh-container"></div>
    </div>
{% endblock %}

{% block include_js %}
    <script>
        window.aseaman.ready(function(exports) {
            exports.initCompareACNH('compare-acnh-container');
        });
    </script>
{% endblock %}
